{
  "name": "desafio_getnet",
  "version": "1.0.0",
  "description": "Criação de script de testes automatizados para atender as camadas de API's e Web com Cypress.",
  "main": "index.js",
  "scripts": {
    "cy:open": "npx cypress open",
    "cy:headless": "npx cypress run",
    "cy:api": "npx cypress run --spec cypress/e2e/api",
    "cy:web": "npx cypress run --spec cypress/e2e/web",
    "tesults": "node runner.js"
  },
  "keywords": [
    "Testes",
    "Testes automatizados",
    "api",
    "web"
  ],
  "author": "Adriano Almeida",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^13.11.0",
    "cypress-plugin-api": "^2.11.1",
    "cypress-real-events": "^1.12.0",
    "cypress-tesults-reporter": "^1.4.1",
    "dotenv": "^16.4.5"
  }
}
